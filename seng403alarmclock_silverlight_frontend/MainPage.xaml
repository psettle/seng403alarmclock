                        <Rectangle Fill="Black" RadiusX="5" RadiusY="5"/>
                        <sdk:Label Content="Snooze" FontSize="35" Foreground="#242424" HorizontalAlignment="Center"/>
                        <Border BorderBrush="#242424" BorderThickness="1" CornerRadius="5"/>
